{
  "name": "blog-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "tailwind:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/tailwindpresets.css",
    "start": "node ./bin/www",
    "devStart": "nodemon ./bin/www",
    "serverStart": "DEBUG=express-blog-server:* npm run devStart",
    "dev": "concurrently \"npm run tailwind:css -- -w\" \"npm run devStart\""
  },
  "dependencies": {
    "autoprefixer": "^10.4.15",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^6.11.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-errors": "~1.6.3",
    "luxon": "^3.4.3",
    "mongoose": "^7.5.0",
    "morgan": "~1.9.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "postcss": "^8.4.29",
    "postcss-cli": "^10.1.0",
    "serve-favicon": "^2.5.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "ejs-lint": "^2.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4"
  }
}
